<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <link rel="stylesheet" href="~/layui/css/layui.css" media="all" asp-append-version="true" />
    @*<link rel="stylesheet" href="~/layui/css/layui.css" media="all" asp-append-version="true" />*@
    <style type="text/css">
        .layui-checkbox-disbaled, .layui-checkbox-disbaled i {
            border-color: #e2e2e2 !important;
            background-color: #8eca9e !important;
        }

        .layui-table-link {
            cursor: pointer
        }
    </style>
    <environment include="Development">
        <link rel="stylesheet" href="~/css/public.css" media="all" asp-append-version="true" />
        <link href="~/css/icon.css" rel="stylesheet" media="all" asp-append-version="true" />
    </environment>

    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/public.min.css" media="all" asp-append-version="true" />
        <link href="~/css/icon.min.css" rel="stylesheet" media="all" asp-append-version="true" />
    </environment>

    @RenderSection("head", false)
</head>
<body class="childrenBody">
    @RenderBody()
    <script type="text/javascript" src="~/layui/layui.js" asp-append-version="true"></script>
    @RenderSection("Scripts", false)
</body>
</html>