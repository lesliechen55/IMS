
@model UserInfoSelectDataResponse
@{
    Layout = "_Layout";
}
<blockquote class="layui-elem-quote">
    <form class="layui-form">
        <div class="layui-inline">
            <input type="hidden" id="page" name="page" value="1" />
            <input type="hidden" id="limit" name="limit" value="10" />
            <div class="layui-input-inline">
                <input type="text" class="layui-input" id="userId" name="userId" placeholder="请输入用户ID" />
            </div>
            <div class="layui-input-inline">
                <input type="text" class="layui-input" id="userName" name="userName" placeholder="请输入用户名" />
            </div>
            <div class="layui-input-inline">
                <select id="apiState" name="apiState">
                    <option value="">请选择状态</option>
                    @foreach (var (key, value) in Model.ListApiState)
                    {
                        <option value="@key">@value</option>
                    }
                </select>
            </div>
            <div class="layui-input-inline">
                <input type="text" class="layui-input" id="startTime" name="startTime" placeholder="请选择开始时间">
            </div>
            <div class="layui-input-inline">
                <input type="text" class="layui-input" id="endTime" name="endTime" placeholder="请选择结束时间">
            </div>
            <a class="layui-btn" id="search">搜索</a>
        </div>
    </form>
</blockquote>
<table id="table" class="layui-table" lay-filter="table"></table>

<script type="text/html" id="toolbar">
    <div class="layui-btn-container">
        <button class="layui-btn" lay-event="add"><i class="layui-icon">&#xe608;</i> 添加</button>
        @*<button class="layui-btn layui-btn-danger" lay-event="update"><i class="layui-icon">&#xe642;</i> 编辑 </button>*@
    </div>
</script>

@section Scripts{
    <environment include="Development">
        <script src="~/js/trackApi/userInfo/index.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/trackApi/userInfo/index.min.js" asp-append-version="true"></script>
    </environment>
}

