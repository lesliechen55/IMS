@{
    Layout = "_Layout";
}
<blockquote class="layui-elem-quote">
    <form class="layui-form">
        <div class="layui-inline">
            <input type="hidden" id="page" name="page" value="1" />
            <input type="hidden" id="limit" name="limit" value="10" />
            <div class="layui-input-inline">
                <input type="text" id="orderId" name="orderId" class="layui-input" placeholder="请输入询价单ID" />
            </div>
            <div class="layui-input-inline">
                <input type="text" id="startTime" name="startTime" class="layui-input" placeholder="请输入开始日期" />
            </div>
            <div class="layui-input-inline">
                <input type="text" id="endTime" name="endTime" class="layui-input" placeholder="请输入结束日期" />
            </div>
            <a class="layui-btn" id="search">搜索</a>
        </div>
    </form>
</blockquote>
<table id="table" class="layui-table" lay-filter="table"></table>
<script type="text/html" id="toolbar">
</script>
@section Scripts{
    <environment include="Development">
        <script src="~/js/freight/inquiryManage/statusLog.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/freight/inquiryManage/statusLog.min.js" asp-append-version="true"></script>
    </environment>
}