@model IframeTransferData<CompanyEditResponse>
@{
    Layout = "_Layout";
}
<div class="layui-form">
    <input type="hidden" name="id" value="@Model.Id" />
    <div class="layui-form-item">
        <label class="layui-form-label">渠道额度</label>
        <div class="layui-input-block">
            <input type="number" name="limit" lay-verify="required" placeholder="请输入渠道额度" autocomplete="off" class="layui-input" value="@Model.Data.Limit">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label">企业官网</label>
        <div class="layui-input-block">
            <input type="text" name="url" placeholder="请输入企业官网" required lay-verify="required" autocomplete="off" class="layui-input" value="@Model.Data.Url">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label" style="margin-top: -11px;">营业执照社会信用代码</label>
        <div class="layui-input-block">
            <input type="text" name="code" placeholder="请输入营业执照号/社会信用代码" required autocomplete="off" class="layui-input" lay-verify="required" value="@Model.Data.Code">
        </div>
    </div>
    <div class="layui-form-item layui-form-text">
        <label class="layui-form-label">手机号码</label>
        <div class="layui-input-block">
            <input type="text" name="phone" placeholder="请输入手机号码" required autocomplete="off" class="layui-input" lay-verify="required" value="@Model.Data.Phone">
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-input-block">
            <button class="layui-btn" lay-submit lay-filter="form">立即提交</button>
            <button class="layui-btn layui-btn-danger" id="close">关闭</button>
        </div>
    </div>
</div>
@section Scripts{
    <environment include="Development">
        <script src="~/js/freight/company/edit.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/freight/company/edit.min.js" asp-append-version="true"></script>
    </environment>
}
